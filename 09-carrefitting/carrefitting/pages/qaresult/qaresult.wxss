/* pages/qaresult/qaresult.wxss */
.container {
  position: relative;
  margin: 20rpx auto;
  padding: 20rpx;
  width: 670rpx;
  height: auto;
  background-color: #000;
  border-radius: 10rpx;
  font-size: 30rpx;
  line-height: 60rpx;
  overflow: hidden;
}

.name {
  font-weight: bold;
  color: #fff;
}

.cont {
  display: flex;
  flex-wrap: wrap;
  color: #999999;
  border-bottom: 1rpx solid #f5f5f5;
}

.image {
  width: 120rpx;
  margin: 7rpx;
}

.icon {
  position: absolute;
  right: 27rpx;
  bottom: 250rpx;
  width: 197rpx;
  height: 161rpx;
}

.b-t {
  border-top: 1rpx solid #f5f5f5;
}

.b-r {
  border-right: 1rpx solid #f5f5f5;
}

.b-b {
  border-bottom: 1rpx solid #f5f5f5;
}

.b-l {
  border-left: 1rpx solid #f5f5f5;
}