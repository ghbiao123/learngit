/* pages/qualityreport/qualityreport.wxss */
page{
  background-size: 750rpx 15rpx;
  background-image: url("data:image/jpeg;base64,");
  background-repeat: repeat-y;
}
.content {
  position: relative;
  height: auto;
  min-height: 100vh;
}

.head {
  position: relative;
  width: 750rpx;
  height: 475rpx;
  overflow: hidden;
}

.bg-up {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 750rpx;
  height: 475rpx;
}


.bg-middle {
  position: absolute;
  top: 475rpx;
  left: 0;
  z-index: -1;
  width: 750rpx;
  height: 260rpx;
  background-size: 750rpx 15rpx;
  background-image: url("data:image/jpeg;base64,");
  background-repeat: repeat-y;
}

.bg-down {
  position: absolute;
  left: 0;
  bottom: 0;
  z-index: -1;
  width: 750rpx;
  height: 603rpx;
}

.copyright {
  position: absolute;
  bottom: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  font-size: 30rpx;
  font-weight: bold;
}

.copyright-line {
  margin: 0 20rpx;
  width: 100rpx;
  height: 2rpx;
  background-color: #707070;
}


/* 主体内容 */
.cont {
  width: 100%;
  padding-bottom: 150rpx;
  /* height: 200rpx; */
}

.cell-card {
  background-color: rgba(0, 0, 0, 0);
}

.cell-item {
  justify-content: center;
  border-bottom: none;
  height: auto;
}

.cell-name {
  padding-left: 100rpx;
  width: 250rpx;
}

.cell-cont {
  padding-top: 30rpx;
  width: 300rpx;
  line-height: 1.5;
}
.image-box{
  display: flex;
  flex-wrap: wrap;
  margin: 0 auto;
  width: 480rpx;
}
.image{
  margin: 20rpx;
  width: 200rpx;
  height: 200rpx;
}