/* pages/otherlist/otherlist.wxss */
.container {
  padding-top: 96rpx;
  background-color: #fff;
}
.searchbar{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  background-color: #fff;
}
.item {
  margin: 0 auto;
  width: 710rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  color: #606060;
  border-bottom: 1rpx solid #f5f5f5;
}