/* pages/searchbar/searchbar.wxss */
page{
  background-color: #fff;
}
.content{
  padding-top: 96rpx;
}
.searchbar{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 96rpx;
  background-color: #fff;
}
.tit-no-bg{
  justify-content: space-between;
}
.tip-content{
  display: flex;
  flex-wrap: wrap;
  padding: 0 0 10rpx;
  margin: 0 auto;
  width: 710rpx;
  height: auto;
}
.tip{
  padding: 0 10rpx;
  width: auto;
  max-width: 670rpx;
  height: 55rpx;
  margin: 25rpx 25rpx 0  0;
  line-height: 55rpx;
  text-align: center;
  font-size: 24rpx;
  background-color: #F7F8FA;
  border-radius: 10rpx;
}

.list{
  padding: 0 20rpx;
  width: 710rpx;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 30rpx;
  border-bottom: 1rpx solid #f5f5f5;
}