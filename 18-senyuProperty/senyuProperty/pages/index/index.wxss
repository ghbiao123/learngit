page {
  min-height: 100vh;
  background-color: #338B56;
}

.head {
  width: 100%;
  height: 173px;
}

.head-img {
  width: 100%;
  height: 100%;
}

.head-cont {
  justify-content: space-between;
  align-items: center;
}

.userinfo {
  width: 90%;
  height: 140rpx;
}

.userinfo-name {
  padding-bottom: 35rpx;
  font-size: 20px;
  font-family: PingFang HK-Semibold, PingFang HK;
  font-weight: 600;
  color: #494E51;
  line-height: 23px;
}

.userinfo-phone {
  font-size: 16px;
  font-family: PingFang HK-Medium, PingFang HK;
  font-weight: 500;
  color: #8D959A;
  line-height: 19px;
}

.more {
  width: 12rpx;
  height: 24rpx;
}

.menulist {
  position: relative;
  margin: 50rpx auto 0;
  padding: 34rpx 50rpx 44rpx;
  width: 510rpx;
  height: 80rpx;
  background-color: #D5E9F9;
  border-radius: 20rpx;
}

.menutitle {
  font-size: 32rpx;
  font-family: PingFang HK-Semibold, PingFang HK;
  font-weight: 600;
  color: #494E51;
  line-height: 38rpx;
}

.menudesc {
  margin-top: 16rpx;
  font-size: 24rpx;
  font-family: Roboto-Light, Roboto;
  font-weight: 300;
  color: #8D959A;
  line-height: 28rpx;
}
.menuicon{
  position: absolute;
  right: 20rpx;
  top: -35rpx;
  width: 188rpx;
  height: 188rpx;
}
