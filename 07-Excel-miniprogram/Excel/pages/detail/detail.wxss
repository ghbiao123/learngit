.cell{
  display: flex;
  justify-content: space-between;
  margin: 10rpx auto 0;
  padding: 20rpx;
  width: 670rpx;
  /* height: 50rpx; */
  line-height: 50rpx;
  background-color: #fff;
  border-radius: 10rpx;
}
.cell-key{
  width: 370rpx;
  font-size: 30rpx;
  color: #999999;
}
.cell-value{
  width: 300rpx;
  text-align: right;
  font-size: 30rpx;
  color: #1A1A1A;
  font-weight: bold;
}
page{
  background-color: #f5f5f5;
}