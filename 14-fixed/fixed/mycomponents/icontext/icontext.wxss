.icontext{
  padding: 10rpx 7rpx 0;
  width: 120rpx;
  text-align: center;
  overflow: hidden;
}
.icontext-hover{
  background-color: #f5f5f5;
}
.icon{
  width: 85rpx;
  height: 85rpx;
}
.text{
  min-width: 85rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}